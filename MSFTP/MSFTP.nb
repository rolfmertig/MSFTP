Cell["Tests ", "Section"],

Cell["\<\
you can of course set $myUserName,  $myPassword and $myServer here \
explicitly.
If not, dialogs will pop up:\
\>", "Text"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{"!", 
     RowBox[{"ValueQ", "[", "$myServer", "]"}]}], ",", " ", 
    RowBox[{"$myServer", " ", "=", " ", 
     RowBox[{
     "InputString", "[", 
      "\"\<Enter the ssh (sftp) server URL, without http://\>\"", "]"}]}]}], 
   "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{"!", 
     RowBox[{"ValueQ", "[", "$myUserName", "]"}]}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"$myUserName", " ", "=", "  ", 
     RowBox[{"InputString", "[", 
      RowBox[{"\"\<Enter your username on \>\"", "<>", "$myServer"}], 
      "]"}]}]}], "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"If", "[", 
   RowBox[{
    RowBox[{"!", 
     RowBox[{"ValueQ", "[", "$myPassword", "]"}]}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"$myPassword", " ", "=", " ", 
     RowBox[{"InputString", "[", 
      RowBox[{
      "\"\<Enter your password for \>\"", "<>", "$myUserName", "<>", 
       "\"\< on \>\"", "<>", "$myServer"}], "]"}]}]}], "]"}], ";"}]}], "Input"],

