      RowBox[{"\"\<UserName\>\"", " ", "\[Rule]", " ", "$myUserName"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"\"\<Password\>\"", " ", "\[Rule]", "$myPassword"}]}], 
